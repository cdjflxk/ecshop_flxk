<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<meta name="Keywords" content="{$keywords}" />
<meta name="Description" content="{$description}" />
<!-- TemplateBeginEditable name="doctitle" -->
<title>{$page_title}</title>
<!-- TemplateEndEditable --><!-- TemplateBeginEditable name="head" --><!-- TemplateEndEditable -->
<link rel="shortcut icon" href="favicon.ico" />
<link rel="icon" href="animated_favicon.gif" type="image/gif" />
<link href="{$ecs_css_path}" rel="stylesheet" type="text/css" />

{* 包含脚本文件 *}
{insert_scripts files='common.js,lefttime.js'}
<script type="text/javascript" src="js/action.js"></script>
<script type="text/javascript">
  {foreach from=$lang.js_languages item=item key=key}
    var {$key} = "{$item}";
  {/foreach}
</script>
</head>
<body>
<!-- #BeginLibraryItem "/library/page_header.lbi" --><!-- #EndLibraryItem -->
<!-- #BeginLibraryItem "/library/search_form.lbi" --><!-- #EndLibraryItem -->
<!--主体内容 start-->
<div class="Area clearfix">
   <div class="HomeLeft f_l">  
   <div class="PublicBorderTwo">
    <h3 class="F14B">{$lang.your_choice}
    </h3>
    <div class="clearfix Conwid4" id="pick">
   <!--{foreach from = $picks item=pick }-->
     <a href="{$pick.url}">{$pick.name}</a><br />
     <!--{/foreach}-->
  </div>
  </div>  
 <!-- TemplateBeginEditable name="左边区域" -->
   <!-- #BeginLibraryItem "/library/categorys.lbi" --><!-- #EndLibraryItem -->
  <!-- #BeginLibraryItem "/library/history.lbi" --><!-- #EndLibraryItem -->
  <!-- #BeginLibraryItem "/library/promotion_info.lbi" --><!-- #EndLibraryItem -->
 <!-- TemplateEndEditable -->
 </div>
<div class="HomeRight f_r">
   <!-- #BeginLibraryItem "/library/ur_here.lbi" --><!-- #EndLibraryItem -->
  <!--搜索结果 start-->
  <div class="PublicBorderTwo">
    <h3 class="F14B">{$lang.search_result} ({$count})
   <!--{if $count > 4}--><a href="{$url}" style="color:#000;"><span class="MorePic"></span></a><!--{/if}-->
    </h3>
    <div class="clearfix Conwid4">
  <!--{foreach from=$pickout_goods item=goods name=goods}-->
     <div class="goodsbox">
      <div class="imgbox"><a href="{$goods.url}"><img src="{$goods.thumb}" alt="{$goods.name|escape:html}" /></a></div>
      <a href="{$goods.url}" title="{$goods.name|escape:html}">{$goods.short_name}</a><br />
      <!-- {if $goods.promote_price neq ""} -->
      {$lang.promote_price}<b class="price">{$goods.promote_price}</b><br>
      <!-- {else}-->
      {$lang.shop_price}<b class="price">{$goods.shop_price}</b><br>
      <!--{/if}-->
     </div>
    <!--{/foreach}-->
  </div>   
  </div>
  <!--搜索结果 end--> 
   <div class="PublicBorderTwo">
    <h3 class="F14B">{$lang.pick_out}
    </h3>
    <div class="clearfix Conwid4">
  <table width="100%" border="0" cellpadding="5" cellspacing="1" bgcolor="#dddddd" style="margin-bottom:8px;">
      <!--{foreach from=$condition item=caption}-->
      <tr>
        <td bgcolor="#e5ecfb">
          <img src="images/note1.gif" alt="no alt" style="position: relative;top:3px;" />&nbsp;&nbsp;<font class="f2">{$caption.name}</font></td>
      </tr>
      <!--{foreach from=$caption.cat item=cat}-->
      <tr>
        <td bgcolor="#ffffff">&nbsp;&nbsp;<strong>{$cat.cat_name}</strong></td>
      </tr>
      <tr>
        <td bgcolor="#ffffff">&nbsp;&nbsp;
          <!--{foreach from = $cat.list item=list}-->
          &nbsp;&nbsp;<a href="{$list.url}">{$list.name}</a>
          <!--{/foreach}-->
        </td>
      </tr>
      <!--{/foreach}-->
      <!--{/foreach}-->
    </table>
  </div>
  </div>  
 </div>
</div>
<!--主体内容 end-->
